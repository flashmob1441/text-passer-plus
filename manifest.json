{
  "manifest_version": 3,
  "name": "Text-Passer+",
  "version": "1.1.0",
  "description": "Save the text and paste it into selected elements on different websites.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "icons": {
  }
}